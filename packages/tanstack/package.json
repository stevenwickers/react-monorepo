    "jsdom": "^27.4.0",
    "ts-node": ">=10",
    "typescript": ">=5",
    "typescript-eslint": "^8.51.0",
    "vite": ">=5",
    "vite-plugin-dts": ">=4",
    "vitest": "^4.0.16"
